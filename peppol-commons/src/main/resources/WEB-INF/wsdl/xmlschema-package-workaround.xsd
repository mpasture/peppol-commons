<schema xmlns="http://www.w3.org/2001/XMLSchema" 
        targetNamespace="http://www.w3.org/2001/XMLSchema"
        xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
        jaxb:version="2.0">
  <annotation>
    <documentation>
      This is a workaround for the problem of the created "org.w3._2001.xmlschema" package.
      
      Source: https://jaxb.java.net/guide/Customizing_Java_packages.html#Tip__get_rid_of_the_org_w3__2001_xmlschema_package
    </documentation>
    <appinfo>
      <jaxb:schemaBindings>
        <jaxb:package name="com.helger.peppol.xsd" />
      </jaxb:schemaBindings>
    </appinfo>
  </annotation>
</schema>