<bindings xmlns="http://java.sun.com/xml/ns/jaxb"
          xmlns:xs="http://www.w3.org/2001/XMLSchema"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns:wsa="http://www.w3.org/2005/08/addressing" 
          xmlns:inheritance="http://jaxb2-commons.dev.java.net/basic/inheritance"
          xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd"
          xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" 
          extensionBindingPrefixes="xjc"
          version="2.1">
  <globalBindings generateValueClass="true">
    <javaType name="java.time.LocalDateTime"
              xmlType="xs:dateTime"
              parseMethod="com.helger.commons.datetime.PDTWebDateHelper.getLocalDateTimeFromXSD"
              printMethod="com.helger.commons.datetime.PDTWebDateHelper.getAsStringXSD" />
  </globalBindings>
  <bindings schemaLocation="../resources/WEB-INF/wsdl/Identifiers-1.0.xsd">
    <schemaBindings>
      <package name="com.helger.peppol.identifier" />
    </schemaBindings>
    <bindings node="//xs:complexType[@name='ParticipantIdentifierType']">
      <inheritance:implements>com.helger.peppol.identifier.generic.participant.IMutableParticipantIdentifier</inheritance:implements>
    </bindings> 
    <bindings node="//xs:complexType[@name='DocumentIdentifierType']">
      <inheritance:implements>com.helger.peppol.identifier.generic.doctype.IMutableDocumentTypeIdentifier</inheritance:implements>
    </bindings> 
    <bindings node="//xs:complexType[@name='ProcessIdentifierType']">
      <inheritance:implements>com.helger.peppol.identifier.generic.process.IMutableProcessIdentifier</inheritance:implements>
    </bindings> 
  </bindings>
  <!-- Copied from com.sun.tools.ws.processor.modeler.wsdl.PseudoSchemaBuilder from jaxws-tools-2.2.3 -->
  <!-- See http://jaxb.java.net/nonav/2.2.3u1/docs/vendorCustomizations.html for details -->
  <bindings scd="x-schema::wsa" if-exists="true">
    <schemaBindings map="false" />
    <!-- this binding applies to an element -->
    <bindings scd="wsa:EndpointReference">
      <class ref="javax.xml.ws.wsaddressing.W3CEndpointReference" />
    </bindings>
    <bindings scd="wsa:ReplyTo">
      <class ref="javax.xml.ws.wsaddressing.W3CEndpointReference" />
    </bindings>
    <bindings scd="wsa:FaultTo">
      <class ref="javax.xml.ws.wsaddressing.W3CEndpointReference" />
    </bindings>
    <bindings scd="wsa:From">
      <class ref="javax.xml.ws.wsaddressing.W3CEndpointReference" />
    </bindings>
    <!-- this binding applies to a type - note the leading '~' -->
    <bindings scd="~wsa:EndpointReferenceType">
      <class ref="javax.xml.ws.wsaddressing.W3CEndpointReference" />
    </bindings>
  </bindings>
  <bindings schemaLocation="../resources/WEB-INF/wsdl/ServiceMetadataPublishingTypes-1.0.xsd">
    <schemaBindings>
      <package name="com.helger.peppol.smp" />
    </schemaBindings>
  </bindings>
  <bindings schemaLocation="../resources/WEB-INF/wsdl/bdx-smp-201605.xsd">
    <schemaBindings>
      <package name="com.helger.peppol.bdxr" />
    </schemaBindings>
    <bindings node="//xs:complexType[@name='ParticipantIdentifierType']">
      <inheritance:implements>com.helger.peppol.identifier.generic.participant.IMutableParticipantIdentifier</inheritance:implements>
    </bindings> 
    <bindings node="//xs:complexType[@name='DocumentIdentifierType']">
      <inheritance:implements>com.helger.peppol.identifier.generic.doctype.IMutableDocumentTypeIdentifier</inheritance:implements>
    </bindings> 
    <bindings node="//xs:complexType[@name='ProcessIdentifierType']">
      <inheritance:implements>com.helger.peppol.identifier.generic.process.IMutableProcessIdentifier</inheritance:implements>
    </bindings> 
  </bindings>
  <bindings schemaLocation="../resources/WEB-INF/wsdl/oasis-200401-wss-wssecurity-utility-1.0.xsd">
    <schemaBindings>
      <package name="com.helger.peppol.wssec" />
    </schemaBindings>
  </bindings>
</bindings>
